 screens\r\n\r\n@media (min-width: 600px) {\r\n    header {\r\n        position: sticky;\r\n        top: 0;\r\n    }\r\n    \r\n    \r\n    #main-header {\r\n        bottom: 0;\r\n    }\r\n}\r\n*/\r\n\r\n/*\r\nhamburger menu\r\n*/\r\n.hamburger-menu {\r\n    border: none;\r\n    padding: .5vh;\r\n    margin: 0 3vw 0 0;\r\n    height: 100%;\r\n    cursor: pointer;\r\n    background-color: transparent;\r\n}\r\n\r\n.hamburger-box {\r\n    width: 6.2vh;\r\n    height: 100%;\r\n    position: relative;\r\n    display: inline-block;\r\n}\r\n\r\n.hamburger-bar, .hamburger-bar::before,\r\n.hamburger-bar::after {\r\n    display: inline-block;\r\n    position: absolute;\r\n    width: 100%;\r\n    height: .8vh;\r\n    left: 0;\r\n    background-color: #dd0000;\r\n}\r\n\r\n.hamburger-bar::before,\r\n.hamburger-bar::after {\r\n    content: \"\";\r\n}\r\n\r\n.hamburger-bar {\r\n    top: 50%;\r\n    transform: translateY(-50%);\r\n    transition: background-color 0.5s 1.225s;\r\n}\r\n\r\n.hamburger-bar::before {\r\n    transform: translateY(-2vh);\r\n}\r\n\r\n.hamburger-bar::after {\r\n    transform: translateY(2vh);\r\n}\r\n\r\n/*\r\nmain menu\r\n*/\r\nnav {\r\n    position: absolute;\r\n    top: 7.5vh;\r\n    width: 100vw;\r\n    height: 92.5vh;\r\n    overflow: hidden;\r\n    z-index: 20;\r\n}\r\n\r\n.menu {\r\n    list-style: none;\r\n    padding: 17vh 0 20vh 0;\r\n    background-color: #353030;\r\n    width: 100%;\r\n    height: 100%;\r\n    margin: 0;\r\n    display: flex;\r\n    flex-flow: column;\r\n    justify-content: space-around;\r\n    transform: translateX(100vw);\r\n    transition: transform 1s .9s ease-in;\r\n}\r\n\r\n.nav-elem {\r\n    text-decoration: none;\r\n    color: #fff;\r\n    font-size: 1.5em;\r\n    text-transform: uppercase;\r\n}\r\n.menu>li {\r\n    display: flex;\r\n    justify-content: center;\r\n}\r\n\r\n.menu hr {\r\n    width: 30vw;\r\n}\r\n\r\n.nav-elem, .menu hr {\r\n    transform: translateX(100vw);\r\n    transition: transform 1s .4s ease-in-out;\r\n}\r\n/*\r\nslide menu - folling each after each efect\r\n*/\r\n.menu hr:first-of-type {\r\n    transition-delay: .5s;\r\n}\r\n\r\n.works {\r\n    transition-delay: .6s;\r\n}\r\n\r\n.menu hr:nth-of-type(2) {\r\n    transition-delay: .7s;\r\n}\r\n\r\n.achievements {\r\n    transition-delay: .8s;\r\n}\r\n\r\n.menu hr:nth-of-type(3) {\r\n    transition-delay: .9s;\r\n}\r\n\r\n.contact {\r\n    transition-delay: 1s;\r\n}\r\n\r\n/*\r\ntogling hamburger menu\r\n*/\r\n.active .menu {\r\n    transform: translateX(0);\r\n    transition: transform 1s .2s ease-out;\r\n}\r\n\r\n.active .nav-elem, .active hr  {\r\n    transform: translateX(0);\r\n}\r\n\r\n.close .hamburger-bar {\r\n    background-color: transparent;\r\n    animation: main-bar 1.5s 1.5s;\r\n}\r\n\r\n.close .hamburger-bar::before {\r\n    animation: before 3.5s;\r\n    transform: translateY(0) rotate(45deg);\r\n}\r\n\r\n.close .hamburger-bar::after {\r\n    animation: after 3.5s;\r\n    transform: translateY(0) rotate(-45deg);\r\n}\r\n\r\n@keyframes main-bar {\r\n    0% {transform: scaleX(1);}\r\n    20% {transform: scaleX(1);}\r\n    35% {transform: scaleX(0.1);}\r\n    50% {transform: scaleX(1);}\r\n    100% {transform: scaleX(1);}\r\n}\r\n\r\n@keyframes before {\r\n    0% { transform: translateY(-2vh); }\r\n    15% { transform: translateY(-2.7vh); }\r\n    20% { transform: translateY(-2.7vh); }\r\n    35% { transform: translateY(0); }\r\n    50% { transform:  rotate(0); }\r\n    100% { transform: rotate(45deg); }\r\n}\r\n\r\n@keyframes after {\r\n    0% { transform: translateY(2vh); }\r\n    15% { transform: translateY(2.7vh); }\r\n    20% { transform: translateY(2.7vh); }\r\n    35% { transform: translateY(0); }\r\n    50% { transform:  rotate(0); }\r\n    100% { transform: rotate(-45deg); }\r\n}\r\n\r\n/*\r\nmain content\r\n*/\r\n\r\n.section-heading {\r\n    background-color: #fff;\r\n    margin: 5vh 0;\r\n    height: 7vh;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    text-transform: uppercase;\r\n    color: #353030;\r\n    font: 300 2em/1em 'Martel Sans', sans-serif;\r\n}\r\n\r\n.full-text {\r\n    display: none;\r\n}\r\n\r\n.biography-person, \r\n.works-person, \r\n.achievements-person,\r\n.contact-info {\r\n    margin: 0 3vw 5vh 3vw;\r\n    display: flex;\r\n    align-content: flex-start;\r\n}\r\n\r\n/*\r\nbiography and achievements section\r\n*/\r\n.portrait, .work-example {\r\n    width: 40%;\r\n}\r\n\r\n.portrait {\r\n    border-radius: 50%;\r\n    border: .2em solid #fff;\r\n    align-self: center;\r\n}\r\n\r\n.work-example {\r\n    align-self: flex-start;\r\n}\r\n\r\n.tetiana .portrait, .tetiana .work-example {\r\n    margin-right: 1vw;\r\n}\r\n\r\n.mykhailo .portrait, .mykhailo .work-example {\r\n    margin-left: 1vw;\r\n}\r\n\r\n.short-text {\r\n    display: flex;\r\n    flex-wrap: wrap;\r\n}\r\n\r\n.tetiana .short-text {\r\n    justify-content: flex-end;\r\n}\r\n\r\n.mykhailo .short-text {\r\n    justify-content: flex-start;\r\n}\r\n\r\n.name-uppercase {\r\n    text-transform: uppercase;\r\n    font-weight: 500;\r\n}\r\n\r\n.short-text-p {\r\n    margin: 0;\r\n}\r\n\r\n.tetiana .short-text-p {\r\n    text-align: end;\r\n}\r\n\r\n.show-more {\r\n    border: none;\r\n    margin: 2vh 0;\r\n    padding: .7vh 3vw;\r\n    background-color: #dd0000;\r\n    color: #fff;\r\n    font-size: 1em;\r\n    text-align: center;\r\n    cursor: pointer;\r\n}\r\n/*\r\nworks section\r\n*/\r\n#works-tetiana {\r\n    margin: 0 3vw;\r\n}\r\n\r\n.show-more.tetiana-works {\r\n    margin: 0;\r\n}\r\n\r\n/*\r\nfooter\r\n*/\r\nfooter {\r\n    background-color: #fff;\r\n    color: #353030;\r\n    font-size: .8em;\r\n    text-align: end;\r\n    padding: 5vw;\r\n}\r\n\r\n.author {\r\n    text-decoration: none;\r\n    color: #dd0000;\r\n}\r\n\r\n.author:hover {\r\n    text-decoration: underline;\r\n}"},"lineCount":380}},"error":null,"hash":"fc50db29fc68bc80fddf07e1c5fc3d72","cacheData":{"env":{}}}